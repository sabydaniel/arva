<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="action_ship_report_xlsx" model="ir.actions.report">
        <field name="name">Shipping Report XLSX</field>
        <field name="model">ship.report.wizard</field>
        <field name="report_type">xlsx</field>
        <field name="report_name">ship_reports.ship_report_xlsx</field>
        <field name="report_file">ship_reports.ship_report_xlsx</field>
        <field name="print_report_name">
            'Shipping_Report_%s_%s' % (object.from_date, object.to_date)
        </field>
    </record>

</odoo>
