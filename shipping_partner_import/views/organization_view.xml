<odoo>


    <record id="view_shipping_organization_form" model="ir.ui.view">
        <field name="name">shipping.organization.form</field>
        <field name="model">shipping.organization</field>
        <field name="arch" type="xml">
            <form string="Organization">
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="code"/>
                        <field name="receivable_acct"/>
                        <field name="payable_acct"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>


    <record id="view_shipping_organization_tree" model="ir.ui.view">
        <field name="name">shipping.organization.tree</field>
        <field name="model">shipping.organization</field>
        <field name="arch" type="xml">
            <tree string="Organizations">
                <field name="name"/>
                <field name="code"/>
                <field name="receivable_acct"/>
                <field name="payable_acct"/>
            </tree>
        </field>
    </record>

</odoo>
