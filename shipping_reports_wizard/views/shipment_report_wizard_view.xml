<odoo>
    <record id="view_shipment_report_wizard_form" model="ir.ui.view">
        <field name="name">shipment.report.wizard.form</field>
        <field name="model">shipment.report.wizard</field>
        <field name="arch" type="xml">
            <form string="Shipment Report">
                <group>
                    <field name="date_from"/>
                    <field name="date_to"/>
                </group>

                <footer>
                    <button string="Send"
                            name="action_send"
                            type="object"
                            class="btn-primary"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>

        </field>
    </record>
</odoo>
